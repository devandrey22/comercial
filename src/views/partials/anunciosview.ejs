          <% 
      if(clients.lenght!=0) {
        var i=1;
        clients.reverse().forEach(function(data) { 
        
          %>
          <tr>
            <th scope="row"><%=i %></th>
            <td><%= data.Email %></td>
            <td><%= data.Repetition %></td>
         
          </tr>
          <% i++; })} %>  
        </tbody>
