  <div class="cssshowlaptopcampanas"> 
    <div style="display: flex; flex-direction: column; max-width: 100%; flex-wrap: wrap; margin-top: 10px; margin-left: 250px; margin-right: 50px; font-family: 'Raleway', sans-serif; ">
    <table class="table table-striped table-hover">
      <thead>
        
        <tr>
          <th scope="col">#</th>
          <th scope="col">Date</th>
          <th scope="col">Nombre</th>
          <th scope="col">Email</th>
          <th scope="col">Mensaje</th>
          
        </tr>
      </thead>
      <tbody>
        <% 
        if(clients.lenght!=0) {
          var i=1;
          clients.reverse().forEach(function(data) { 
          
            %>
        <tr>
          <th scope="row"><%=i %></th>
          <td><%= data.Fecha %></td>
          <td><%= data.Nombre %></td>
          <td><%= data.Email %></td>
          <td><%= data.Mensaje %></td>

        </tr>
        <% i++; })} %>  
      </tbody>
    </table>
  
    </div>
