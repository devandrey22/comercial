<div class="cssshowlaptopcampanas"> 

  <div style="display: flex; flex-direction: column; max-width: 100%; flex-wrap: wrap; margin-top: 10px; margin-left: 250px; margin-right: 50px; font-family: 'Raleway', sans-serif; ">
      
    
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">ID</th>
        <th scope="col">Nombre</th>
        <th scope="col">Email</th>
        <th scope="col">Telefono</th>
        <th scope="col">Titulo</th>
        <th scope="col">Plan</th>
        <th scope="col">Fechas a anunciar</th>
        <th scope="col">Zonas</th>
        <th scope="col">Fecha solicitud</th>
        <th scope="col">Status</th>
      </tr>
    </thead>
    <tbody>
          <% 
          if(clients.lenght!=0) {
            var i=1;
            clients.reverse().forEach(function(data) { 
            
              %>
         <tr>
        
        <th scope="row"><%=i %> </th>
        <td><%= data.ID %></td>
        <td><%= data.Nombre %></td>
        <td><%= data.Email %></td>
        <td><%= data.Telefono %></td>
        <td><%= data.titulo %></td>
        <td><%= data.plan %></td>
        <td><%= data.fechas %></td>
        <td><%= data.zonas %></td>
        <td><%= data.fechadesolicitud %></td>
        <td><%= data.status %></td>

      </tr>
      <% i++; })} %>      </tbody>
  </table>

  </div>
</div>
  


