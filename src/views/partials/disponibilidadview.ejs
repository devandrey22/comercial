     </div>
</div>


<div class="card"
          style="display: flex; flex-direction: row; max-width: 100%; flex-wrap: wrap; margin-top: 10px; margin-left: 250px; margin-right: 50px; font-family: 'Raleway', sans-serif; align-items: center; align-content: center; ">
         
<table class="table table-light table-striped text-center">
     <thead>
       <tr>
         <th scope="col">Dia</th>
         <th scope="col">Ocupados</th>
       
       </tr>
     </thead>
     <tbody>
                    
          <% 
          if(clients.lenght!=0) {
            var i=1;
            clients.forEach(function(data) { 
            
              %>
       <tr>
         <td><%= data.Dia %></td>
         <td><%= data.Ocupados %></td>
       </tr>
       
       <% i++; })} %>
     </tbody>
   </table>
</div>
